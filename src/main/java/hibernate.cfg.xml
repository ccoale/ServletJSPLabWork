<?xml version="1.0" encoding="UTF-8"?>

<hibernate-configuration>
	<session-factory>
		<property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
		<property name="hibernate.connection.password">123456</property>
		<property name="hibernate.connection.url">jdbc:mysql://localhost:3306/protein_tracker</property>
		<property name="hibernate.connection.username">appuser</property>
		<property name="hibernate.default_schema">protein_tracker</property>
		<property name="hiberate.dialect">org.hibernate.dialect.MySQLDialect</property>
		<property name="hiberate.show_sql">true</property>
		<mapping resource="main/java/RegisteredUser.hbm.xml" />
		<mapping resource="main/java/DataRecord.hbm.xml" />
	</session-factory>
</hibernate-configuration>